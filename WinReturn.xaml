<Window x:Class="SnakeLocal.WinReturn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeLocal"
        mc:Ignorable="d"
        Title="WinReturn" Height="320" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" ></RowDefinition>
            <RowDefinition Height="*" ></RowDefinition>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" x:Name="return" FontSize="30" Content="Заново" Height="150" Width="300" Click="return_Click" ></Button>
        <Button x:Name="exit" Grid.Row="1" FontSize="30" Content="Выход" Height="155" Width="300" Click="exit_Click" ></Button>
    </Grid>
</Window>
